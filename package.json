{
  "name": "algebra-calculator",
  "version": "1.0.0",
  "description": "Algebra calculator with React frontend and Node backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "npm install --prefix client",
    "install-all": "npm install && npm run client-install"
  },
  "keywords": [
    "algebra",
    "calculator",
    "react",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "algebrite": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mathjs": "^12.1.0",
    "node-fetch": "^2.6.9",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
